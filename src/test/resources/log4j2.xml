<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="Console">
			<PatternLayout>
				<Pattern>%highlight{[%-5p]} [%t] - %m%n</Pattern>
			</PatternLayout>
		</Console>
		<File name="File" fileName="target/logs/google-cloud-automation.log" append="false">
			<PatternLayout>
				<Pattern>[%d{yyyy-MM-dd HH:mm:ss}] [%-5p] %l %m%n</Pattern>
			</PatternLayout>
		</File>
	</Appenders>
	<Loggers>
		<Root level="error">
			<AppenderRef ref="Console" />
			<AppenderRef ref="File" />
		</Root>
		<Logger name="webdriver" level="trace" additivity="false">
			<AppenderRef ref="Console" />
		</Logger>
	</Loggers>
</Configuration>